<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.escgame.updategames</string>
    <key>ProgramArguments</key>
    <array>
	<string>/usr/bin/python</string>
        <string>/Library/Scripts/ESC_Scripts/esc-update-games.py</string>
